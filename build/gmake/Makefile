.NOTPARALLEL:

ifndef config
  config=release_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release_x64)
  grpc_lua_config = release_x64
endif
ifeq ($(config),release_x32)
  grpc_lua_config = release_x32
endif
ifeq ($(config),debug_x64)
  grpc_lua_config = debug_x64
endif
ifeq ($(config),debug_x32)
  grpc_lua_config = debug_x32
endif

PROJECTS := grpc_lua

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

grpc_lua:
ifneq (,$(grpc_lua_config))
	@echo "==== Building grpc_lua ($(grpc_lua_config)) ===="
	@${MAKE} --no-print-directory -C . -f grpc_lua.make config=$(grpc_lua_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f grpc_lua.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release_x64"
	@echo "  release_x32"
	@echo "  debug_x64"
	@echo "  debug_x32"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   grpc_lua"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"